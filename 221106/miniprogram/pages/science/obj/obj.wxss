.content{
  position: relative;
  top:200rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  overflow: hidden;
  font-size: 30rpx;
  margin-left: 5%;
  width: 90%;
  color: #89969F;
  z-index: -1;
}
.expand{
  -webkit-line-clamp: 0;
}
.collapse{
  -webkit-line-clamp: 1;
}
